<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tmobtech.tmobbeaconproject" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CameraActivity"
            android:label="@string/title_activity_camera"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.tmobtech.tmobbeaconproject.MainActivity" />
        </activity>
        <activity
            android:name=".PlaceBeaconActivity"
            android:label="@string/title_activity_place_beacon" >
        </activity>
        <activity
            android:name=".MeasureBeaconPowerActivity"
            android:label="@string/title_activity_measure_beacon_power" >
        </activity>
    </application>

</manifest>
